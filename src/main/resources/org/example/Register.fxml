<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.fxControllers.Register">
   <children>
      <TextField fx:id="nameTextField" layoutX="38.0" layoutY="33.0" promptText="Name" />
      <TextField fx:id="surNameTextField" layoutX="38.0" layoutY="74.0" promptText="Surname" />
      <TextField fx:id="addressTextField" layoutX="38.0" layoutY="117.0" promptText="Address" />
      <TextField fx:id="emailTextField" layoutX="38.0" layoutY="165.0" promptText="E-Mail" />
      <DatePicker fx:id="birthDateDatePicker" layoutX="38.0" layoutY="211.0" promptText="BirthDate" />
      <Button fx:id="registerButton" layoutX="263.0" layoutY="317.0" mnemonicParsing="false" onAction="#registerUser" text="Register" />
      <Button fx:id="clearButton" layoutX="361.0" layoutY="317.0" mnemonicParsing="false" onAction="#clearFields" text="Clear" />
      <TextField fx:id="loginTextField" layoutX="256.0" layoutY="104.0" promptText="Login" />
      <TextField fx:id="passwordTextField" layoutX="257.0" layoutY="152.0" promptText="Password" />
   </children>
</AnchorPane>
